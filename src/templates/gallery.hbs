{{#each this}}

  <div class='photo-card'>
    <a class='large-img' href='{{largeImageURL}}'>
      <img
        class='photo'
        src='{{webformatURL}}'
        alt='{{tags}}'
        loading='lazy'
        width='400'
        height='250'
      /></a>
    <div class='info'>
      <p class='info-item'>
        <b class='info-value'>Likes</b>
        <br />{{likes}}
      </p>
      <p class='info-item'>
        <b class='info-value'>Views</b>
        <br />{{views}}
      </p>
      <p class='info-item'>
        <b class='info-value'>Comments</b>
        <br />{{comments}}
      </p>
      <p class='info-item'>
        <b class='info-value'>Downloads</b>
        <br />{{downloads}}
      </p>
    </div>
  </div>
{{/each}}